{
  "className": "Scaffold",
  "na": {
    "body": {
      "className": "CustomScrollView",
      "na": {
        "slivers": [
          {
            "className": "SliverAppBar",
            "na": {
              "title": {
                "className": "Text",
                "pa": [
                  "CustomScrollView模版"
                ]
              }
            }
          },
          {
            "className": "SliverToBoxAdapter",
            "na": {
              "child": {
                "className": "Container",
                "na": {
                  "margin": {
                    "className": "EdgeInsets.symmetric",
                    "na": {
                      "vertical": 15
                    }
                  },
                  "child": {
                    "className": "Text",
                    "pa": [
                      "Flutter Best UI x Fair 最佳实践"
                    ],
                    "na": {
                      "style": {
                        "className": "TextStyle",
                        "na": {
                          "color": "#(Colors.black87)",
                          "fontWeight": "#(FontWeight.w600)",
                          "fontSize": 24
                        }
                      }
                    }
                  }
                }
              }
            }
          },
          {
            "className": "SliverGrid",
            "na": {
              "gridDelegate": {
                "className": "Sugar.sliverGridDelegateWithFixedCrossAxisCount",
                "na": {
                  "crossAxisCount": 2,
                  "mainAxisSpacing": 10.0,
                  "crossAxisSpacing": 10.0,
                  "childAspectRatio": 2.0
                }
              },
              "delegate": {
                "className": "SliverChildBuilderDelegate",
                "pa": [
                  "@(_itemBuilder)"
                ],
                "na": {
                  "childCount": 3
                }
              }
            }
          },
          {
            "className": "SliverList",
            "na": {
              "delegate": {
                "className": "SliverChildBuilderDelegate",
                "pa": [
                  "@(_itemBuilder)"
                ],
                "na": {
                  "childCount": 3
                }
              }
            }
          }
        ]
      }
    }
  },
  "methodMap": {
    "_itemBuilder": {
      "className": "Container",
      "na": {
        "color": "#(Colors.yellow)",
        "child": {
          "className": "Text",
          "pa": [
            "#($index)"
          ]
        },
        "width": 100,
        "height": 100
      }
    }
  }
}