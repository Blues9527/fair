            "top": 8
          }
        },
        "scrollDirection": "#(Axis.vertical)",
        "itemBuilder": {
          "className": "FairWidget",
          "na": {
            "name": "#(card-${hotelData.hashCode})",
            "path": "assets/bundle/lib_best_flutter_ui_template_hotel_listbuilder_item.fair.json",
            "data": {
              "fairProps": "%(jsonEncode)"
            }
          }
        }
      }
    }
  },
  "methodMap": {}
}