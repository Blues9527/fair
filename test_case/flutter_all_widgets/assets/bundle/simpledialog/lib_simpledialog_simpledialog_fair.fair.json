{
  "className": "Container",
  "na": {
    "child": {
      "className": "SimpleDialog",
      "na": {
        "title": {
          "className": "Text",
          "pa": [
            "提示"
          ]
        },
        "children": [
          {
            "className": "Container",
            "na": {
              "height": 80,
              "alignment": "#(Alignment.center)",
              "child": {
                "className": "Text",
                "pa": [
                  "确认删除吗？"
                ]
              }
            }
          },
          {
            "className": "Divider",
            "na": {
              "height": 1
            }
          },
          "%(_getCancel)",
          {
            "className": "Divider",
            "na": {
              "height": 1
            }
          },
          "%(_getOk)"
        ]
      }
    }
  },
  "methodMap": {
    "_getOk": {
      "className": "TextButton",
      "na": {
        "child": {
          "className": "Text",
          "pa": [
            "确认"
          ]
        },
        "onPressed": {
          "pa": [
            "ok"
          ]
        }
      }
    },
    "_getCancel": {
      "className": "TextButton",
      "na": {
        "child": {
          "className": "Text",
          "pa": [
            "取消"
          ]
        },
        "onPressed": {
          "pa": [
            "cancel"
          ]
        }
      }
    }
  }
}