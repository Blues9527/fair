{
  "className": "Container",
  "na": {
    "child": {
      "className": "ScrollConfiguration",
      "na": {
        "behavior": "%(_getBehavior)",
        "child": "%(_getChild)"
      }
    }
  },
  "methodMap": {
    "_getChild": {
      "className": "ListView.separated",
      "na": {
        "itemBuilder": {
          "className": "Text",
          "pa": [
            "#(Item$index)"
          ]
        },
        "separatorBuilder": {
          "className": "Divider"
        },
        "itemCount": 50
      }
    },
    "_getBehavior": {
      "className": "ScrollBehavior"
    }
  }
}