{
  "className": "Scaffold",
  "na": {
    "appBar": {
      "className": "AppBar",
      "na": {
        "title": {
          "className": "Text",
          "pa": [
            "Fair BottomSheet"
          ]
        }
      }
    },
    "body": {
      "className": "Center"
    },
    "bottomSheet": {
      "className": "BottomSheet",
      "na": {
        "onClosing": "@(onClosing)",
        "builder": "@(_bottomWidget)"
      }
    }
  },
  "methodMap": {
    "_bottomWidget": {
      "className": "Container",
      "na": {
        "height": 300,
        "color": "#(Colors.amber)",
        "alignment": "#(Alignment.centerLeft)",
        "child": {
          "className": "Text",
          "pa": [
            "BottomSheet in Scaffold"
          ]
        }
      }
    }
  }
}